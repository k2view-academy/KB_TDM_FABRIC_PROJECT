          replace_sequences:
            schema: any
            mandatory: false
          scheduler:
            schema: any
            mandatory: false
          task_created_by:
            schema: any
            mandatory: false
          retention_period_type:
            schema: any
            mandatory: false
          retention_period_value:
            schema: any
            mandatory: false
