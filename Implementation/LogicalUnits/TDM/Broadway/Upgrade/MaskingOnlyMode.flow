tags: Upgrade
stages:
  Masking Only:
    actors:
      Add Masking Only Indocator:
        parent: DbCommand
        in:
          interface:
            const: TDM
          sql:
            const: |-
              update @TDMDB_SCHEMA@.tdm_general_parameters
              set param_value = substr(param_value, 0, length(param_value) ) || ',"masking_only":true}' where param_name = 'tdm_gui_params'
