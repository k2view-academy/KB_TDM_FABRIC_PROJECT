stages:
  Table's Details:
    actors:
      Get Table Details:
        parent: InnerFlow
        height: 296
        in:
          flowName:
            const: GetTableLevelDetails
          ref_instance_id:
            external: refInstanceId
            schema: string
            mandatory: false
        out:
          result:
            schema: '#ref'
  Get Load Flow if Exists:
    actors:
      Get List of Table's Fields:
        parent: LuFunction
        in:
          functionName:
            const: fnGetTableFields
          dbInterfaceName:
            link: Get Table Details/result/target_interface
            schema: string
            mandatory: false
          SchemaName:
            link: Get Table Details/result/target_schema
            schema: string
            mandatory: false
          tableName:
            link: Get Table Details/result/target_table_name
            schema: string
            mandatory: false
        out:
          result:
            schema: '#ref'
  List of Fields:
    last: 1
    actors:
      Build Array of Fields:
        parent: ArrayBuilder
        in:
          input:
            link:
              path: Get List of Table's Fields/result/column_name
              iterate: Iterate
              pos: 0
        out:
          array:
            schema: '#ref'
  Input Records:
    actors:
      Records To Be Loaded:
        parent: Const
        in:
          value:
            const: null
            external: inputRecords
            schema: object
        out:
          value:
            schema: object
      Get Commit Size:
        parent: FabricSetRead
        in:
          key:
            const: TDM_REF_UPD_SIZE
            default: false
        out:
          result:
            schema: integer
      Get Load Flow:
        parent: LuFunction
        in:
          functionName:
            const: fnGetTableDefinitions
          interfaceName:
            link: Get Table Details/result/interface_name
            schema: string
            mandatory: false
          schemaName:
            link: Get Table Details/result/schema_name
            schema: string
            mandatory: false
          tableName:
            link: Get Table Details/result/ref_table_name
            schema: string
            mandatory: false
          attrName:
            const: load_flow
            schema: string
            mandatory: false
        out:
          result:
            schema: string
  Update Table Current Count:
    actors:
      Update Count:
        parent: Peek
        in:
          flowName:
            const: UpdateTableCurrentCount
          commitSize:
            link: Get Commit Size/result
            schema: integer
            mandatory: false
          taskExeId:
            link: Get Table Details/result/task_execution_id
            schema: integer
            mandatory: false
          tableId:
            link: Get Table Details/result/ref_table_id
            schema: integer
            mandatory: false
          values:
            link: Records To Be Loaded/value
        out:
          values:
            schema: '#ref'
  Load By Query:
    else: false
    dependsOn: Update Table Current Count
    actors:
      No Load Flow?:
        parent: Equals
        condition: result
        in:
          a:
            link: Get Load Flow/result
            schema: string
          b:
            const: ''
            schema: string
      Error Handler For Load By Query:
        parent: ErrorHandler
        error: result
        in:
          config:
            const:
            - exceptionKey: java.lang.Exception
              conditions:
                message: ''
              actions:
                suppress: false
                log: true
                flowName: PopulateTableErrorsForTableLevel
      load Table By Query:
        parent: InnerFlow
        in:
          flowName:
            const: LoadTableByQuery
          innerFlowClose:
            const: false
          targetInterface:
            external: targetInterface
            schema: string
            mandatory: false
          targetSchema:
            external: targetSchema
            schema: string
            mandatory: false
          targetTable:
            external: targetTable
            schema: string
            mandatory: false
          inputRecords:
            link: Update Count/values
            schema: '#ref'
            mandatory: false
          fieldsArray:
            link: Build Array of Fields/array
            schema: '#ref'
            mandatory: false
        out:
          result:
            schema: integer
    split: '--------------------'
  Load Flow:
    else: true
    dependsOn: Update Table Current Count
    actors:
      Error Handler For Load By Custom Flow:
        parent: ErrorHandler
        error: result
        in:
          config:
            const:
            - exceptionKey: java.lang.Exception
              conditions:
                message: ''
              actions:
                suppress: false
                log: true
                flowName: PopulateTableErrorsForTableLevel
      Run Load Flow:
        parent: InnerFlow
        in:
          flowName:
            const: null
            link: Get Load Flow/result
          innerFlowClose:
            const: false
          targetInterface:
            external: targetInterface
            schema: string
            mandatory: false
          targetSchema:
            external: targetSchema
            schema: string
            mandatory: false
          targetTable:
            external: targetTable
            schema: string
            mandatory: false
          inputRecords:
            link: Update Count/values
            schema: '#ref'
            mandatory: false
          fieldsArray:
            link: Build Array of Fields/array
            schema: '#ref'
            mandatory: false
        out:
          result:
            schema: integer
  Update Processed Count:
    actors:
      Update Count?:
        parent: NotEquals
        condition: result
        in:
          a:
            link:
            - load Table By Query/result
            - Run Load Flow/result
            schema: integer
          b:
            const: -1
            schema: integer
      Update Table's Processed Count:
        parent: DbCommand
        in:
          interface:
            const: TDM
          sql:
            const: "update @TDMDB_SCHEMA@.task_ref_exe_stats\r\nset number_of_processed_records\
              \ = ${recCount}\r\nwhere task_execution_id = ${taskExeId} \r\nand task_ref_table_id\
              \ = ${tableId}"
          taskExeId:
            link: Get Table Details/result/task_execution_id
            schema: integer
            mandatory: false
          tableId:
            link: Get Table Details/result/ref_table_id
            schema: integer
            mandatory: false
          recCount:
            link:
            - load Table By Query/result
            - Run Load Flow/result
            schema: integer
            mandatory: false
schemas:
  Get Table Details.out.result:
    type: object
    properties:
      ref_table_id:
        type: integer
      ref_table_name:
        type: string
      source_env:
        type: string
      task_type:
        type: string
      interface_name:
        type: string
      schema_name:
        type: string
      task_execution_id:
        type: integer
      task_id:
        type: integer
      filter_type:
        type: string
      table_filter:
        type: string
      filter_parameters:
        type: string
      retention_period_type:
        type: string
      retention_period_value:
        type: integer
      target_interface:
        type: string
      target_schema:
        type: string
      target_table_name:
        type: string
      number_of_records_to_process:
        type: integer
      retention:
        type: integer
  Get List of Table's Fields.out.result:
    type: array
    items:
      type: object
      properties:
        column_name:
          type: string
        column_type:
          type: string
  Build Array of Fields.out.array:
    type: array
    items:
      type: string
  Update Count.out.values:
    type: array
    items:
      type: object
      properties:
        contract_ref_id:
          type: integer
        offer_ref_id:
          type: integer
        offer_contract_description:
          type: string
  load Table By Query.in.inputRecords:
    type: array
    items: {
      }
  load Table By Query.in.fieldsArray:
    type: array
    items:
      type: string
  Run Load Flow.in.inputRecords:
    type: array
    items: {
      }
  Run Load Flow.in.fieldsArray:
    type: array
    items:
      type: string
