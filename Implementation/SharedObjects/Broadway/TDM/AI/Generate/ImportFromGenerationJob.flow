tags: TDM,TDMGenerateData
stages:
  Get Globals:
    actors:
      AI_ENVIRONMENT:
        parent: FabricSetRead
        in:
          key:
            const: AI_ENVIRONMENT
      AI_DB_K2SYSTEM1:
        parent: FabricSetRead
        in:
          key:
            const: AI_DB_INTERFACE
      TDM_TASK_EXE_ID:
        parent: FabricSetRead
        in:
          key:
            const: TDM_TASK_EXE_ID
      AI_DB_ENTITIES:
        parent: FabricSetRead
        in:
          key:
            const: AI_DB_INTERFACE
      Get BE_ID:
        parent: FabricSetRead
        in:
          key:
            const: BE_ID
      TDMDB_SCHEMA:
        parent: FabricSetRead
        in:
          key:
            const: TDMDB_SCHEMA
  'Build IIDS ':
    actors:
      trainingExcludedTables:
        parent: InnerFlow
        in:
          flowName:
            const: trainingExcludedTables
          luName:
            external: luName
            schema: string
            mandatory: false
        out:
          tables:
            schema: string
      IID:
        parent: Const
        in:
          value:
            const: null
            external: IID
        out:
          value:
            schema: string
      BE_NAME:
        parent: DbFetchField
        in:
          interface:
            const: TDM
          sql:
            const: select be_name from ${@TDMDB_SCHEMA}.business_entities where be_id
              = ?
          TDMDB_SCHEMA:
            link: TDMDB_SCHEMA/result
            schema: string
            mandatory: false
          params:
            link: Get BE_ID/result
        out:
          result:
            schema: string
      trainingIncludedTables1:
        parent: InnerFlow
        in:
          flowName:
            const: trainingIncludedTables
          luName:
            external: luName
            schema: any
            mandatory: false
        out:
          tables:
            schema: string
  Split IID:
    actors:
      JsonParser:
        parent: JsonParser
        in:
          stream:
            link: IID/value
        out:
          object:
            schema: '#ref'
  'initaite Task Execution Entities   ':
    actors:
      Regex:
        parent: Regex
        in:
          pattern:
            const: \s*"(\d+)"
          input:
            link: JsonParser/object/iid
  Get The Fabric TDM root IID:
    actors:
      id:
        parent: Const
        in:
          value:
            const: null
            link:
              path: Regex/strings
              iterate: First
        out:
          value:
            schema: string
  Build Tdm Eid:
    actors:
      buildTDMEidForAI:
        parent: InnerFlow
        in:
          flowName:
            const: buildTDMEidForAI
          Env:
            link: AI_ENVIRONMENT/result
            schema: any
            mandatory: false
          IID:
            link: id/value
            schema: any
            mandatory: false
          ExecutionID:
            link: TDM_TASK_EXE_ID/result
            schema: any
            mandatory: false
        out:
          string:
            schema: string
  Adjust Inputs For Task Execution Entites:
    actors:
      'initaiteTaskExecutionEntities ':
        parent: InnerFlow
        in:
          flowName:
            const: initaiteTaskExecutionEntities
          IID:
            link: id/value
            schema: string
            mandatory: false
          luName:
            external: luName
            schema: string
            mandatory: false
          Status:
            const: completed
            schema: string
            default: true
            mandatory: false
          SourceIID:
            const: ''
            link: buildTDMEidForAI/string
            schema: string
            default: true
            mandatory: false
          TargetIID:
            const: ''
            link: id/value
            schema: string
            default: true
            mandatory: false
      isParamCoupling:
        parent: Const
        in:
          value:
            const: null
            external: isParamCoupling
        out:
          value:
            schema: string
  Import to Fabric:
    transactional: false
    dependsOn: Adjust Inputs For Task Execution Entites
    actors:
      Included List:
        parent: EqualsIgnoreCase
        condition: result
        in:
          b:
            const: ''
            schema: string
      Initation Error Handle1:
        parent: ErrorHandler
        error: result
        in:
          config:
            const:
            - exceptionKey: java.lang.Exception
              conditions:
                message: ''
              actions:
                suppress: false
                log: false
                flowName: PopulateTableErrorsWithFailed
      'Import ':
        parent: DbCommand
        in:
          interface:
            const: fabric
          sql:
            const: MDB_IMPORT ${luName}.${IID} WITH INTERFACE_NAME=${INTERFACE} EXCLUDED_TABLES=${excludedTables}
              REMOTE_IID=${REMOTE}
          IID:
            link: buildTDMEidForAI/string
            schema: string
            mandatory: false
          REMOTE:
            external: IID
            schema: string
            mandatory: false
          luName:
            external: luName
            schema: string
            mandatory: false
          excludedTables:
            link: trainingExcludedTables/tables
            schema: string
            mandatory: false
          INTERFACE:
            link: AI_DB_K2SYSTEM1/result
            schema: string
            mandatory: false
    split: '--------------------'
  Included:
    else: true
    transactional: false
    dependsOn: Adjust Inputs For Task Execution Entites
    actors:
      Initation Error Handle12:
        parent: ErrorHandler
        error: result
        in:
          config:
            const:
            - exceptionKey: java.lang.Exception
              conditions:
                message: ''
              actions:
                suppress: false
                log: false
                flowName: PopulateTableErrorsWithFailed
      Import With Included:
        parent: DbCommand
        in:
          interface:
            const: fabric
          sql:
            const: MDB_IMPORT ${luName}.${IID} WITH INTERFACE_NAME=${INTERFACE} INCLUDED_TABLES=${includedTables}
              EXCLUDED_TABLES=${excludedTables} REMOTE_IID=${REMOTE}
          IID:
            link: buildTDMEidForAI/string
            schema: string
            mandatory: false
          REMOTE:
            external: IID
            schema: string
            mandatory: false
          luName:
            external: luName
            schema: string
            mandatory: false
          excludedTables:
            link: trainingExcludedTables/tables
            schema: string
            mandatory: false
          INTERFACE:
            link: AI_DB_K2SYSTEM1/result
            schema: string
            mandatory: false
          includedTables:
            link: trainingIncludedTables1/tables
            schema: any
            mandatory: false
  Mapping:
    transactional: true
    actors:
      PopulateTDMAIGenIIDMapping_Actor2:
        parent: PopulateTDMAIGenIIDMapping_Actor
        in:
          ' env_name':
            link: AI_ENVIRONMENT/result
          ' task_execution_id':
            link: TDM_TASK_EXE_ID/result
          ' root_imported_lui':
            link: buildTDMEidForAI/string
          ' imported_lui':
            link: buildTDMEidForAI/string
          ' root_lu_name ':
            external: luName
          ' lu_name':
            external: luName
          ' root_generated_iid ':
            external: IID
          ' generated_iid':
            external: IID
  Get Sync Off:
    transactional: true
    actors:
      'FabricGet ':
        parent: FabricGet
        in:
          luType:
            const: null
            external: luName
          iid:
            const: null
            link:
              path: buildTDMEidForAI/string
              pos: 0
            default: false
          syncMode:
            const: 'OFF'
  Update Lu Params:
    transactional: true
    actors:
      Initation Error Handle11:
        parent: ErrorHandler
        error: result
        in:
          config:
            const:
            - exceptionKey: java.lang.Exception
              conditions:
                message: ''
              actions:
                suppress: false
                log: false
                flowName: PopulateTableErrorsWithFailed
      'Enrichemnt Lu Params ':
        parent: LuFunction
        in:
          functionName:
            const: fnGenerationLuParams
          luName:
            external: luName
            schema: string
            mandatory: false
          iid:
            link: buildTDMEidForAI/string
            schema: string
            mandatory: false
  TDM Tables:
    transactional: true
    actors:
      FABRIC_TDM_ROOT:
        parent: DbCommand
        in:
          interface:
            const: fabric
          sql:
            const: insert into ${@luName}.fabric_tdm_root (K2_TDM_EID,SOURCE_ENV,IID,TASK_EXECUTION_ID)
              values (${tdm_eid}, ${env}, ${iid},${task_exe_id})
          tdm_eid:
            link: buildTDMEidForAI/string
            schema: string
            mandatory: false
          iid:
            link: id/value
            schema: string
            mandatory: false
          luName:
            external: luName
            schema: string
            mandatory: false
          task_exe_id:
            link: TDM_TASK_EXE_ID/result
            schema: string
            mandatory: false
          env:
            link: AI_ENVIRONMENT/result
            schema: string
            mandatory: false
      'tdm_be_iids         ':
        parent: DbLoad
        in:
          interface:
            const: fabric
          command:
            const: upsert
          schema:
            const: null
            external: luName
          table:
            const: TDM_BE_IIDS
          fields:
            const:
            - IID
            - SOURCE_ENV
            - BE_ID
            - BE_NAME
            - ROOT_IID
          keys:
            const:
            - IID
            - SOURCE_ENV
            - BE_ID
            - ROOT_IID
          dialect:
            const: sqlite
          IID:
            link: id/value
            schema: string
            mandatory: false
          SOURCE_ENV:
            link: AI_ENVIRONMENT/result
            schema: string
            mandatory: false
          BE_ID:
            link: Get BE_ID/result
            schema: string
            mandatory: false
          BE_NAME:
            link: BE_NAME/result
            schema: any
            mandatory: false
          ROOT_IID:
            link: id/value
            schema: any
            mandatory: false
  To Lower Case:
    transactional: false
    actors:
      'schema name ':
        parent: Lowercase
        in:
          string:
            external: luName
  Get Root Table:
    transactional: false
    actors:
      Get Root Table Name:
        parent: DbCommand
        in:
          interface:
            const: null
            link: AI_DB_ENTITIES/result
          sql:
            const: |-
              SELECT DISTINCT
                  relname AS root_table
              FROM
                  pg_class
              WHERE
                  oid NOT IN (
                      SELECT
                          conrelid
                      FROM
                          pg_constraint
                      WHERE
                          contype = 'f'
                      AND
                          connamespace = (SELECT oid FROM pg_namespace WHERE nspname = '${@schemaName}')  -- Replace 'your_schema_name' with your schema name
                  )
              AND
                  relnamespace = (SELECT oid FROM pg_namespace WHERE nspname = '${@schemaName}')  -- Replace 'your_schema_name' with your schema name
              AND
                  oid NOT IN (
                      SELECT DISTINCT
                          conrelid
                      FROM
                          pg_constraint
                      WHERE
                          contype = 'f'
                      AND
                          connamespace = (SELECT oid FROM pg_namespace WHERE nspname = '${@schemaName}')  -- Replace 'your_schema_name' with your schema name
                  )
              AND
                  relkind = 'r'
                  AND relname NOT LIKE '_k2\_%'   -- Excluding specific k2 tables
              ORDER BY
                  relname;
          schemaName:
            link: schema name /string
            schema: string
            mandatory: false
        out:
          result:
            schema: '#ref'
  Delete Cascade from Entities DB:
    last: 1
    transactional: false
    actors:
      Initation Error Handle111:
        parent: ErrorHandler
        error: result
        in:
          config:
            const:
            - exceptionKey: java.lang.Exception
              conditions:
                message: ''
              actions:
                suppress: false
                log: false
                flowName: PopulateTableErrorsWithFailed
      Delete Imported From DB:
        parent: DbCommand
        in:
          interface:
            const: null
            link: AI_DB_ENTITIES/result
          sql:
            const: 'delete from ${@schemaName}.${@tableName} where __iid=${imported_iid} '
          schemaName:
            external: luName
            schema: string
            mandatory: false
          tableName:
            link:
              path: Get Root Table Name/result/root_table
              iterate: Iterate
            schema: string
            mandatory: false
          imported_iid:
            external: IID
            schema: string
            mandatory: false
  'Is Params Coupling   ':
    transactional: false
    dependsOn: Delete Cascade from Entities DB
    actors:
      'True':
        parent: EqualsIgnoreCase
        condition: result
        in:
          b:
            const: 'true'
            schema: string
          a:
            link: isParamCoupling/value
      ExportIidForParams:
        parent: InnerFlow
        in:
          flowName:
            const: ExportIidForParams
          luName:
            external: luName
            schema: string
            mandatory: false
          IID:
            link: buildTDMEidForAI/string
            schema: string
            mandatory: false
    split: '--------------------'
  'Else     ':
    else: true
    transactional: false
    dependsOn: Delete Cascade from Entities DB
  'check if load ':
    transactional: false
    actors:
      loadIndicator:
        parent: Const
        in:
          value:
            const: null
            external: loadIndicator
            schema: boolean
        out:
          value:
            schema: boolean
  No Load:
    dependsOn: 'check if load '
    actors:
      'EqualsIgnoreCase false ':
        parent: EqualsIgnoreCase
        condition: result
        in:
          b:
            const: 'false'
            schema: string
          a:
            link: loadIndicator/value
    split: '--------------------'
  'Else   ':
    else: true
    dependsOn: 'check if load '
  Populate Task Execution Entites:
    dependsOn: No Load
    actors:
      Initation Error Handle:
        parent: ErrorHandler
        error: result
        in:
          config:
            const:
            - exceptionKey: java.lang.Exception
              conditions:
                message: ''
              actions:
                suppress: false
                log: false
                flowName: PopulateTableErrorsWithFailed
      PopulateTaskExecutionEntities_Actor1:
        parent: PopulateTaskExecutionEntities_Actor
        in:
          idType:
            const: ENTITY
    split: '--------------------'
  'Do Nothing   ':
    dependsOn: 'Else   '
schemas:
  JsonParser.out.object:
    type: object
    properties:
      env:
        type: string
      iid:
        type: object
        properties:
          customer_id:
            type: string
      exec_id:
        type: integer
  Get Root Table Name.out.result:
    type: array
    items:
      type: object
      properties:
        root_table:
          type: string
