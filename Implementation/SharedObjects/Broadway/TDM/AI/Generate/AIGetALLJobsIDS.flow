tags: CleanUp
stages:
  Start:
    actors:
      AI_DB_K2SYSTEM:
        parent: FabricSetRead
        in:
          key:
            const: AI_DB_INTERFACE
            default: false
      Get k2system Schema Name:
        parent: InnerFlow
        in:
          flowName:
            const: Getk2SystemSchemaName
        out:
          result:
            schema: string
      AI_DB_ENTITIES:
        parent: FabricSetRead
        in:
          key:
            const: AI_DB_INTERFACE
      'Space ID ':
        parent: SysEnv
        in:
          name:
            const: SPACE_NAME
        out:
          value:
            schema: string
      Tenant ID:
        parent: SysEnv
        in:
          name:
            const: TENANT_NAME
        out:
          value:
            schema: string
  Get Namespaces:
    transactional: false
    actors:
      kubeGetNamespaces:
        parent: InnerFlow
        in:
          flowName:
            const: kubeGetNamespaces
        out:
          result:
            schema: '#ref'
      namespaces:
        parent: StringFormat
        in:
          format:
            const: -${space_id}-${tenant_id}
          space_id:
            link: Space ID /value
            schema: '#ref'
            mandatory: false
          tenant_id:
            link: Tenant ID/value
            schema: string
            mandatory: false
  'Substring ':
    transactional: false
    actors:
      Regex find space and tenant names:
        parent: Regex
        in:
          pattern:
            const: null
            link: namespaces/string
          input:
            link:
              path: kubeGetNamespaces/result
              iterate: Iterate
  'Build Array ':
    last: 1
    else: false
    transactional: false
    dependsOn: 'Substring '
    actors:
      IsNull1:
        parent: IsNull
        condition: result
        in:
          value:
            link:
              path: Regex find space and tenant names/strings
              iterate: Iterate
    split: '--------------------'
  Stage 1:
    last: 1
    else: true
    transactional: false
    dependsOn: 'Substring '
    actors:
      ArrayBuilder namespaces:
        parent: ArrayBuilder
        in:
          input:
            link:
              path: kubeGetNamespaces/result
              iterate: Iterate
        out:
          array:
            schema: '#ref'
  close:
    last: 1
    transactional: false
  build array:
    transactional: false
    actors:
      Const:
        parent: Const
        in:
          value:
            const: null
            link: ArrayBuilder namespaces/array
        out:
          value:
            schema: '#ref'
  find ids:
    actors:
      Regex:
        parent: Regex
        in:
          pattern:
            const: ^tdmml(\d+)-[a-zA-Z0-9]+-[a-zA-Z0-9]+-.*
          input:
            link:
              path: Const/value
              iterate: Iterate
  build ids:
    last: 1
    actors:
      ArrayBuilder ids:
        parent: ArrayBuilder
        in:
          input:
            link: Regex/strings
        out:
          array:
            external: array
            schema: '#ref'
  Stage 4: {
    }
  Stage 5: {
    }
