tags: TDM,TDMAI ML
stages:
  Get Globals:
    actors:
      ParamsExcludeTables1:
        parent: InnerFlow
        in:
          flowName:
            const: ParamsExcludeTables
          luName:
            external: luName
            schema: string
            mandatory: false
        out:
          tables:
            schema: string
      ParamsIncludedTables1:
        parent: InnerFlow
        in:
          flowName:
            const: ParamsIncludedTables
          lu_name:
            external: luName
            schema: string
            editor:
              id: com.k2view.mTableKey
            mandatory: false
        out:
          string:
            schema: string
      TDM DB:
        parent: Const
        in:
          value:
            const: TDM
            schema: string
        out:
          value:
            schema: string
  Export:
    actors:
      ErrorHandler:
        parent: ErrorHandler
        error: result
        in:
          config:
            const:
            - exceptionKey: java.lang.Exception
              conditions:
                message: ''
              actions:
                suppress: false
                log: true
                flowName: PopulateTableErrorsWithFailed
                number_of_retries: ''
                retry_interval: ''
      MDB_EXPORT luNameIID:
        parent: DbCommand
        in:
          interface:
            const: fabric
          sql:
            const: MDB_EXPORT ${luName}.${IID} WITH INTERFACE_NAME=${Interfacename}
              INCLUDED_TABLES=${includedTables} EXCLUDED_TABLES=${excludedTables}
              FK='true'
          luName:
            external: luName
            schema: string
            mandatory: false
          IID:
            external: IID
            schema: string
            mandatory: false
          excludedTables:
            link: ParamsExcludeTables1/tables
            schema: string
            mandatory: false
          includedTables:
            link: ParamsIncludedTables1/string
            schema: string
            mandatory: false
          Interfacename:
            link: TDM DB/value
            schema: string
            mandatory: false
  Stage 1: {
    }
