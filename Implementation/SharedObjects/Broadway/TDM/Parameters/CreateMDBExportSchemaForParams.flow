tags: TDM
stages:
  'Get Inputs ':
    actors:
      TDM Interface:
        parent: Const
        in:
          value:
            const: TDM
            schema: string
        out:
          value:
            schema: string
      ParamsExcludeTables1:
        parent: InnerFlow
        in:
          flowName:
            const: ParamsExcludeTables
          luName:
            external: luName
            schema: string
            mandatory: false
        out:
          tables:
            schema: string
      ParamsIncludedTables1:
        parent: InnerFlow
        in:
          flowName:
            const: ParamsIncludedTables
          lu_name:
            external: luName
            schema: string
            editor:
              id: com.k2view.mTableKey
            mandatory: false
        out:
          string:
            schema: string
  'MDB EXPORT Schema ':
    actors:
      ErrorHandler:
        parent: ErrorHandler
        error: result
        in:
          config:
            const:
            - exceptionKey: java.lang.Exception
              conditions:
                message: ''
              actions:
                suppress: false
                log: false
                flowName: ''
                number_of_retries: ''
                retry_interval: ''
      MDB_EXPORT LuName:
        parent: DbCommand
        in:
          interface:
            const: fabric
          sql:
            const: MDB_EXPORT ${luName} WITH INTERFACE_NAME=${interfaceName} INCLUDED_TABLES=${includedTables}
              EXCLUDED_TABLES=${excludedTables} FK='true'
          luName:
            external: luName
            schema: string
            mandatory: false
          interfaceName:
            link: TDM Interface/value
            schema: string
            mandatory: false
          excludedTables:
            link: ParamsExcludeTables1/tables
            schema: string
            mandatory: false
          includedTables:
            link: ParamsIncludedTables1/string
            schema: string
            mandatory: false
