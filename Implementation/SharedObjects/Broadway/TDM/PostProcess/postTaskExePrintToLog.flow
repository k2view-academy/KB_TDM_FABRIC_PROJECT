        {
            "stages": [
                {
                    "name": "Get Task Settings",
                    "dependsOnList": [],
                    "actors": [
                        {
                            "name": "Get Task Exe ID",
                            "actorType": {
                                "parentType": "FabricSetRead",
                                "inputs": [
                                    {
                                        "name": "key",
                                        "const": "TDM_TASK_EXE_ID"
                                    }
                                ],
                                "outputs": []
                            }
                        },
                        {
                            "name": "Get Source Env",
                            "actorType": {
                                "parentType": "FabricSetRead",
                                "inputs": [
                                    {
                                        "name": "key",
                                        "const": "TDM_SOURCE_ENVIRONMENT_NAME"
                                    }
                                ],
                                "outputs": []
                            }
                        },
                        {
                            "name": "Get Target Env",
                            "actorType": {
                                "parentType": "FabricSetRead",
                                "inputs": [
                                    {
                                        "name": "key",
                                        "const": "TDM_TAR_ENV_NAME"
                                    }
                                ],
                                "outputs": []
                            }
                        }
                    ]
                }
            ]
        },
        {
            "stages": [
                {
                    "name": "Get Root LU Record from the TDM DB",
                    "dependsOnList": [],
                    "actors": [
                        {
                            "name": "DbFetchFirstRow1",
                            "actorType": {
                                "parentType": "DbFetchFirstRow",
                                "inputs": [
                                    {
                                        "name": "interface",
                                        "const": "TDM"
                                    },
                                    {
                                        "name": "sql",
                                        "const": "Select public.task_execution_list.task_id,\r\n  public.task_execution_list.execution_status,\r\n  public.task_execution_list.num_of_processed_entities,\r\n  public.task_execution_list.num_of_copied_entities,\r\n  public.task_execution_list.num_of_failed_entities\r\nFrom public.task_execution_list\r\nWhere public.task_execution_list.task_execution_id = ? and parent_lu_id is null"
                                    }
                                ],
                                "outputs": [
                                    {
                                        "name": "result",
                                        "schema": {
                                            "type": "object",
                                            "properties": {
                                                "task_id": {
                                                    "type": "integer"
                                                },
                                                "execution_status": {
                                                    "type": "string"
                                                },
                                                "num_of_processed_entities": {
                                                    "type": "integer"
                                                },
                                                "num_of_copied_entities": {
                                                    "type": "integer"
                                                },
                                                "num_of_failed_entities": {
                                                    "type": "integer"
                                                }
                                            }
                                        }
                                    }
                                ]
                            }
                        },
                        {
                            "name": "Const1",
                            "actorType": {
                                "parentType": "Const",
                                "inputs": [
                                    {
                                        "name": "value",
                                        "remark": "Enter your first name to be included in the message printed to the log upon completion of the post-execution process.\n",
                                        "schema": {
                                            "type": "string"
                                        },
                                        "const": null
                                    }
                                ],
                                "outputs": [
                                    {
                                        "name": "value",
                                        "schema": {
                                            "type": "string"
                                        }
                                    }
                                ]
                            }
                        }
                    ]
                }
            ]
        },
        {
            "stages": [
                {
                    "name": "Print to Log",
                    "dependsOnList": [],
                    "actors": [
                        {
                            "name": "Logger1",
                            "actorType": {
                                "parentType": "Logger",
                                "inputs": [
                                    {
                                        "name": "message",
                                        "const": "The post-execution process has been successfully completed.\r\n\r\nTask Details:\r\n\r\nTask ID: ${1}\r\nTask Execution ID: ${0}\r\nTask Execution Status: ${2}\r\nProcessed Entities: ${3}\r\nSuccessful Entities: ${4}\r\nFailed Entities: ${5}\r\n\r\nThank you, ${6}, for overseeing this process."
                                    },
                                    {
                                        "name": "level",
                                        "const": "info"
                                    }
                                ],
                                "outputs": []
                            }
                        }
                    ]
                }
            ]
        },
        {
            "stages": [
                {
                    "name": "Stage 1",
                    "dependsOnList": [],
                    "actors": []
                }
            ]
        }
    ],
