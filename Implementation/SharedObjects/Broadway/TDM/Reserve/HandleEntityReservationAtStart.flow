tags: TDM
stages:
  'Check For Replace Sequence ':
    actors:
      Get Replace Sequence Indicator:
        parent: FabricSetRead
        in:
          interface:
            default: false
          key:
            const: enable_sequences
            default: false
  Not A Sequence Task:
    dependsOn: 'Check For Replace Sequence '
    actors:
      'false':
        parent: EqualsIgnoreCase
        condition: result
        in:
          b:
            const: 'false'
            schema: string
          a:
            link: Get Replace Sequence Indicator/result
    split: '--------------------'
  Sequence Task:
    else: true
    transactional: false
    dependsOn: 'Check For Replace Sequence '
  Check If Reservation To Be Handled:
    transactional: false
    dependsOn: Not A Sequence Task
    actors:
      Check if Reservation is required:
        parent: Equals
        condition: result
        in:
          a:
            external: reserveInd
            schema: string
          b:
            const: 'true'
            schema: string
    split: '--------------------'
  Stage 15:
    transactional: false
    dependsOn: Sequence Task
  'Check For Delete Only ':
    transactional: false
    dependsOn: Check If Reservation To Be Handled
    actors:
      Get Delete Only Task Indicator:
        parent: FabricSetRead
        in:
          interface:
            default: false
          key:
            const: TDM_DELETE_ONLY_TASK
            default: false
      Get Task Reserve Indicator:
        parent: FabricSetRead
        in:
          interface:
            default: false
          key:
            const: TDM_RESERVE_IND
            default: false
    split: '--------------------'
  Stage 16:
    transactional: false
    dependsOn: Stage 15
  If Not A Delete Only Task:
    transactional: false
    dependsOn: 'Check For Delete Only '
    actors:
      Equals1:
        parent: Equals
        condition: result
        in:
          a:
            link: Get Delete Only Task Indicator/result
            schema: string
          b:
            const: 'false'
            schema: string
    split: '--------------------'
  If A Delete Only Task:
    else: true
    transactional: false
    dependsOn: 'Check For Delete Only '
    actors:
      ReleaseReservedEntity1:
        parent: ReleaseReservedEntity
    split: '--------------------'
  Stage 17:
    transactional: false
    dependsOn: Stage 16
  Check if Reserve Task:
    transactional: false
    dependsOn: If Not A Delete Only Task
    actors:
      Check if A Reservation Task:
        parent: Equals
        condition: result
        in:
          a:
            link: Get Task Reserve Indicator/result
            schema: string
          b:
            const: 'true'
            schema: string
    split: '--------------------'
  Not A Reserve Task:
    else: true
    transactional: false
    dependsOn: If Not A Delete Only Task
    actors:
      Get Target Entity ID:
        parent: FabricSetRead
        in:
          interface:
            default: false
          key:
            const: TARGET_ENTITY_ID
            default: false
      Get Business Entity ID:
        parent: FabricSetRead
        in:
          interface:
            default: false
          key:
            const: BE_ID
            default: false
        out:
          result:
            schema: integer
      Get Target Environment ID:
        parent: FabricSetRead
        in:
          interface:
            default: false
          key:
            const: TARGET_ENVIRONMENT_ID
            default: false
        out:
          result:
            schema: integer
      Get Session User:
        parent: FabricSetRead
        in:
          interface:
            default: false
          key:
            const: USER_NAME
            default: false
      Get User's Fabric Roles:
        parent: FabricSetRead
        in:
          interface:
            default: false
          key:
            const: USER_FABRIC_ROLES
            default: false
      Get TDMDB Schema:
        parent: FabricSetRead
        in:
          interface:
            default: false
          key:
            const: TDMDB_SCHEMA
            default: false
    split: '--------------------'
  Stage 5:
    transactional: false
    dependsOn: If A Delete Only Task
    split: '--------------------'
  Stage 18:
    transactional: false
    dependsOn: Stage 17
  Do nothing:
    transactional: false
    dependsOn: Check if Reserve Task
    split: '--------------------'
  Owner Check:
    transactional: false
    dependsOn: Not A Reserve Task
    actors:
      Get Owner Of Entity:
        parent: DbCommand
        in:
          interface:
            const: TDM
          sql:
            const: "SELECT reserve_owner\r\nFROM ${@schema}.tdm_reserved_entities\r\
              \nWHERE entity_id = ${entityId}\r\nAND be_id = ${beId}\r\nAND env_id\
              \ = ${envId}\r\nAND (reserve_owner != ${userName} \r\n  AND reserve_owner\
              \ NOT IN (${userFabricRoles}))\r\n\r\n"
          entityId:
            link: Get Target Entity ID/result
            schema: string
            mandatory: false
          beId:
            link: Get Business Entity ID/result
            schema: integer
            mandatory: false
          envId:
            link: Get Target Environment ID/result
            schema: integer
            mandatory: false
          userName:
            link: Get Session User/result
            schema: string
            mandatory: false
          userFabricRoles:
            link: Get User's Fabric Roles/result
            schema: string
            mandatory: false
          schema:
            link: Get TDMDB Schema/result
            schema: string
            mandatory: false
        out:
          result:
            schema: '#ref'
    split: '--------------------'
  Stage 6:
    transactional: false
    dependsOn: Stage 5
    split: '--------------------'
  Stage 19:
    transactional: false
    dependsOn: Stage 18
  Stage 10:
    transactional: false
    dependsOn: Do nothing
    actors:
      Equals2:
        parent: Equals
        condition: result
        in:
          a:
            link: Get Replace Sequence Indicator/result
            schema: string
          b:
            const: 'false'
            schema: string
      'Reserve Error Handler ':
        parent: ErrorHandler
        error: result
        in:
          config:
            const:
            - exceptionKey: java.lang.Exception
              conditions:
                message: ''
              actions:
                suppress: false
                log: false
                flowName: PopulateTableErrorsWithFailed
      Reserve Entity:
        parent: ReserveEntity
    split: '--------------------'
  'The Entities Are Saved ':
    transactional: false
    dependsOn: Owner Check
    actors:
      If Record Exists:
        parent: NotNull
        condition: result
        in:
          value:
            link: Get Owner Of Entity/result/reserve_owner
      ErrorHandler3:
        parent: ErrorHandler
        error: result
        in:
          config:
            const:
            - exceptionKey: java.lang.Exception
              conditions:
                message: ''
              actions:
                suppress: false
                log: true
                flowName: PopulateTableErrorsWithFailed
      Throw Exception:
        parent: JavaScript
        in:
          script:
            const: throw "Entity already Reserved by User:" + userName + " and cannot
              be reserved by current user"
          userName:
            link: Get Owner Of Entity/result/reserve_owner
            schema: string
            mandatory: false
    split: '--------------------'
  Stage 7:
    transactional: false
    dependsOn: Stage 6
    split: '--------------------'
  Stage 20:
    transactional: false
    dependsOn: Stage 19
  Stage 1: {
    }
schemas:
  Get Owner Of Entity.out.result:
    type: array
    items:
      type: object
      properties:
        reserve_owner:
          type: string
