tags: TDM
stages:
  Get Input:
    actors:
      StringsToBytes:
        parent: StringsToBytes
        in:
          strings:
            external: value
  Get Reserve Keys:
    actors:
      Get Task Type:
        parent: FabricSetRead
        in:
          key:
            const: TASK_TYPE
            default: false
      Get Replace Sequence Indicator:
        parent: FabricSetRead
        in:
          key:
            const: enable_sequences
            default: false
      Get FILTEROUT_RESERVED:
        parent: FabricSetRead
        in:
          key:
            const: FILTEROUT_RESERVED
            default: false
      BytesToStrings:
        parent: BytesToStrings
        in:
          stream:
            link: StringsToBytes/stream
  Need to Check Reservation?:
    actors:
      Check Reservation?:
        parent: JavaScript
        in:
          script:
            const:
              userCode: "var result = true;\r\nif (taskType == 'EXTRACT' || sequenceInd\
                \ == 'true' || filteroutReserved == 'false') {\r\n    result = false;\r\
                \n}\r\nresult;"
              script: |-
                var result = true;

                if (taskType == 'EXTRACT' || sequenceInd == 'true' || filteroutReserved == 'false') {
                  result = false;
                }

                result;
          taskType:
            link: Get Task Type/result
            schema: string
            mandatory: false
          sequenceInd:
            link: Get Replace Sequence Indicator/result
            schema: string
            mandatory: false
          filteroutReserved:
            link: Get FILTEROUT_RESERVED/result
            schema: string
            mandatory: false
        out:
          result:
            schema: boolean
      Get BE_ID:
        parent: FabricSetRead
        in:
          key:
            const: BE_ID
            default: false
      Get ENV_ID:
        parent: FabricSetRead
        in:
          key:
            const: TARGET_ENVIRONMENT_ID
            default: false
  Reserve Only Tasks:
    dependsOn: Need to Check Reservation?
    actors:
      Reserve Only Task?:
        parent: EqualsIgnoreCase
        condition: result
        in:
          a:
            link: Get Task Type/result
            schema: string
          b:
            const: reserve
            schema: string
      Query Without Check Owner:
        parent: Const
        in:
          value:
            const: 'SELECT reserve_owner FROM ${@schema}.tdm_reserved_entities WHERE
              entity_id = ${entityId} AND be_id = ${beId} AND env_id = ${envId} '
            schema: string
        out:
          value:
            schema: string
    split: '--------------------'
  Not Reserve Only Task:
    else: true
    transactional: false
    dependsOn: Need to Check Reservation?
    actors:
      Query With Check Owner:
        parent: Const
        in:
          value:
            const: SELECT reserve_owner FROM ${@schema}.tdm_reserved_entities WHERE
              entity_id = ${entityId} AND be_id = ${beId} AND env_id = ${envId} AND
              reserve_owner != ${userName}
            schema: string
        out:
          value:
            schema: string
  Prepare Inputs For Query:
    actors:
      Get TDMDB Schema:
        parent: FabricSetRead
        in:
          key:
            const: TDMDB_SCHEMA
            default: false
      BE ID To Number:
        parent: ToNumber
        in:
          value:
            link: Get BE_ID/result
      ENV ID to Number:
        parent: ToNumber
        in:
          value:
            link: Get ENV_ID/result
      Get Session User:
        parent: FabricSetRead
        in:
          key:
            const: USER_NAME
            default: false
  Check if Reserved by other user:
    dependsOn: Prepare Inputs For Query
    actors:
      Check if Reserved?:
        parent: Equals
        condition: result
        in:
          a:
            link: Check Reservation?/result
            schema: boolean
          b:
            const: true
            schema: boolean
      Check If Reserved:
        parent: DbFetchField
        height: 224
        in:
          interface:
            const: TDM
          sql:
            const: null
            link:
            - Query Without Check Owner/value
            - Query With Check Owner/value
          schema:
            link: Get TDMDB Schema/result
            schema: string
            mandatory: false
          entityId:
            link:
              path: BytesToStrings/strings
              iterate: First
            schema: string
            mandatory: false
          beId:
            link: BE ID To Number/number
            schema: integer
            mandatory: false
          envId:
            link: ENV ID to Number/number
            schema: integer
            mandatory: false
          userName:
            link: Get Session User/result
            schema: string
            mandatory: false
        out:
          result:
            schema: string
    split: '--------------------'
  No Need to Check Reservation:
    else: true
    transactional: false
    dependsOn: Prepare Inputs For Query
  Entity Reserved by Other?:
    actors:
      Instance Owner:
        parent: Const
        in:
          value:
            const: null
            link: Check If Reserved/result
            schema: string
        out:
          value:
            schema: string
  Already Reserved:
    dependsOn: Entity Reserved by Other?
    actors:
      If Record Exists:
        parent: NotNull
        condition: result
        in:
          value:
            link: Instance Owner/value
            schema: string
      Filter Out Entry:
        parent: Const
        in:
          value:
            const: false
            schema: boolean
        out:
          value:
            external: value
            schema: boolean
    split: '--------------------'
  Not Reserved:
    else: true
    transactional: false
    dependsOn: Entity Reserved by Other?
    actors:
      Keep Entry:
        parent: Const
        in:
          value:
            const: true
            schema: boolean
        out:
          value:
            external: value
            schema: boolean
