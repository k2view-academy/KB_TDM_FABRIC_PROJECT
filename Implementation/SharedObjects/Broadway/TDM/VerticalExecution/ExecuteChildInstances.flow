tags: TDM
stages:
  Get Orchestrator:
    actors:
      Split Instance List:
        parent: Split
        in:
          delimiter:
            const: null
            external: separator
          string:
            external: instanceList
      Get Orchestrator FlowStringFormat1:
        parent: InnerFlow
        height: 249
        in:
          flowName:
            const: GetOrchestratorFlow
          taskAction:
            external: taskAction
            schema: string
            mandatory: false
        out:
          result:
            schema: '#ref'
  Build IID:
    actors:
      Build Vertical Iids:
        parent: InnerFlow
        height: 238
        in:
          flowName:
            const: BuildVerticalIid
          VERSION_IND:
            external: versionInd
            schema: boolean
            mandatory: false
          SOURCE_ENV:
            external: srcEnv
            schema: string
            mandatory: false
          SOURCE_IID:
            link:
              path: Split Instance List/strings
              iterate: Iterate
            schema: string
            mandatory: false
        out:
          TDM_EID:
            schema: string
  Run Instances in parellel:
    last: 1
    actors:
      Error Handler:
        parent: ErrorHandler
        error: result
      Run Orchestrator:
        parent: InnerFlowAsync
        in:
          flowName:
            const: ExecuteFlow
          threadCount:
            const: 1
          taskAction:
            external: taskAction
            schema: string
            mandatory: false
          luName:
            external: luName
            schema: string
            mandatory: false
          syncMode:
            external: syncMode
            schema: string
            mandatory: false
          reserveInd:
            const: 'false'
            schema: string
            mandatory: false
          iid:
            link: Build Vertical Iids/TDM_EID
            schema: string
            mandatory: false
          execFlowName:
            link: Get Orchestrator FlowStringFormat1/result/orchestrator_flow
            schema: string
            mandatory: false
          isParamCoupling:
            external: isParamCoupling
            schema: string
            mandatory: false
        out:
          results:
            schema: '#ref'
  Wait for completion:
    actors:
      Wait for all IIDs:
        parent: InnerFlowJoin
        in:
          remaining:
            link: Run Orchestrator/remaining
          failures:
            link: Run Orchestrator/failures
schemas:
  Get Orchestrator FlowStringFormat1.out.result:
    type: object
    properties:
      task_action:
        type: string
      orchestrator_flow:
        type: string
  Run Orchestrator.out.results:
    type: array
    items: {
      }
