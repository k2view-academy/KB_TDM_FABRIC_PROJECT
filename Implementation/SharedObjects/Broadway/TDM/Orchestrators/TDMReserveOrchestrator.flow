tags: TDM
stages:
  Stage 1:
    actors:
      Logger1:
        parent: Logger
        in:
          message:
            const: 'Starting TDMReserveOrchestrator for LuName: ${luName} and iid:
              ${iid}'
          level:
            const: info
          luName:
            external: luName
            schema: string
            editor:
              id: com.k2view.logicalUnit
              addEmptyEntry: true
            mandatory: false
          iid:
            external: iid
            schema: string
            mandatory: false
      Split IID And Clone Number:
        parent: SplitIIDAndCloneNumber_Actor
        in:
          iid:
            external: iid
        out:
          UID:
            schema: string
          versionExeID:
            schema: string
      Initiate Entity Status:
        parent: FabricSet
        in:
          key:
            const: ENTITY_STATUS
            default: false
          value:
            const: completed
            default: false
  Stage 2:
    actors:
      Set Lu Name:
        parent: FabricSet
        in:
          key:
            const: LU_TYPE
            default: false
          value:
            const: null
            external: luName
            editor:
              id: com.k2view.logicalUnit
              addEmptyEntry: true
            default: false
      Initiate Target Entity ID after IID Split:
        parent: FabricSet
        remark: The Target Entity ID should be initiated in case the flows fails during
          the next stage and before setting this ID as it is used in the Error handler
        in:
          key:
            const: TARGET_ENTITY_ID
            default: false
          value:
            const: null
            link: Split IID And Clone Number/instanceID
            default: false
      Set IID:
        parent: FabricSet
        in:
          key:
            const: IID
            default: false
          value:
            link: Split IID And Clone Number/instanceID
      Set Source Entity ID:
        parent: FabricSet
        in:
          key:
            const: SOURCE_ENTITY_ID
            default: false
          value:
            const: null
            link: Split IID And Clone Number/UID
            default: false
      Set Clone No:
        parent: FabricSet
        in:
          key:
            const: clone_id
            default: false
          value:
            const: '0'
            default: false
  Stage 3:
    actors:
      Error Handler:
        parent: ErrorHandler
        error: result
      Reserve Entity:
        parent: ReserveEntity
      root_iid:
        parent: FabricSet
        in:
          key:
            const: root_iid
            default: false
          value:
            const: null
            link: Split IID And Clone Number/instanceID
            default: false
      root_lu_name:
        parent: FabricSet
        in:
          key:
            const: root_lu_name
            default: false
          value:
            const: null
            external: luName
            editor:
              id: com.k2view.logicalUnit
              addEmptyEntry: true
            default: false
  Stage 4:
    actors:
      Populate Task Execution Entities:
        parent: PopulateTaskExecutionEntities_Actor
        in:
          idType:
            const: ENTITY
