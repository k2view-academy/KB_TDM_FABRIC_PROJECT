tags: TDM
stages:
  Start:
    actors:
      Print Inputs to Log:
        parent: Logger
        in:
          message:
            const: 'TDMGenerateOrchestrator --> Handling task with following parameters:
              iid: ${iid}, luType: ${luName}, syncMode: ${syncMode}'
          level:
            const: info
          iid:
            external: iid
            schema: string
            mandatory: false
          luName:
            external: luName
            schema: string
            editor:
              id: com.k2view.logicalUnit
              addEmptyEntry: true
            mandatory: false
          syncMode:
            external: syncMode
            schema: string
            mandatory: false
      Get Start Time:
        parent: Now
      Set LU_TYPE:
        parent: FabricSet
        in:
          key:
            const: LU_TYPE
            default: false
          value:
            const: null
            external: luName
            editor:
              id: com.k2view.logicalUnit
              addEmptyEntry: true
            default: false
      Split IID:
        parent: SplitIIDAndCloneNumber_Actor
        in:
          iid:
            external: iid
        out:
          cloneNo:
            schema: string
          UID:
            schema: string
          versionExeID:
            schema: string
      Set INPUT_IID:
        parent: FabricSet
        in:
          key:
            const: INPUT_IID
            default: false
          value:
            const: null
            external: iid
            default: false
  Set Session Variables:
    actors:
      Set TARGET_ENTITY_ID:
        parent: FabricSet
        in:
          key:
            const: TARGET_ENTITY_ID
            default: false
          value:
            const: null
            link: Split IID/instanceID
            default: false
      Format Start Time:
        parent: DateFormat
        in:
          date:
            link: Get Start Time/date
      Set Clone No:
        parent: FabricSet
        in:
          key:
            const: clone_id
            default: false
          value:
            const: '0'
            default: false
      Initiate Entity Status:
        parent: FabricSet
        in:
          key:
            const: ENTITY_STATUS
            default: false
          value:
            const: completed
            default: false
      Set IID:
        parent: FabricSet
        in:
          key:
            const: IID
            default: false
          value:
            const: null
            link: Split IID/instanceID
            default: false
      Set SOURCE_ENTIT_ID:
        parent: FabricSet
        in:
          key:
            const: SOURCE_ENTITY_ID
            default: false
          value:
            const: null
            link: Split IID/UID
            default: false
      TDM_VERSION_TASK_EXECUTION_ID:
        parent: FabricSet
        in:
          key:
            const: TDM_VERSION_TASK_EXECUTION_ID
            default: false
          value:
            link: Split IID/versionExeID
  Get Synthetic Entities:
    actors:
      Set Start Processing Time:
        parent: FabricSet
        in:
          key:
            const: IID_START_DATETIME
            default: false
          value:
            link: Format Start Time/string
      Get Instance:
        parent: FabricGet
        in:
          luType:
            const: null
            external: luName
          syncMode:
            const: null
            external: syncMode
          iid:
            external: iid
      isParamCoupling:
        parent: Const
        in:
          value:
            const: null
            external: isParamCoupling
            schema: string
            editor:
              id: com.k2view.dropdown
              options:
              - 'true'
              - 'false'
        out:
          value:
            schema: string
  'Is Params Coupling ':
    dependsOn: Get Synthetic Entities
    actors:
      'True':
        parent: EqualsIgnoreCase
        condition: result
        in:
          a:
            link: isParamCoupling/value
            schema: string
          b:
            const: 'true'
            schema: string
      ExportIidForParams:
        parent: InnerFlow
        in:
          flowName:
            const: ExportIidForParams
          luName:
            external: luName
            schema: string
            editor:
              id: com.k2view.logicalUnit
              addEmptyEntry: true
            mandatory: false
          IID:
            external: iid
            schema: string
            mandatory: false
    split: '--------------------'
  'False':
    else: true
    dependsOn: Get Synthetic Entities
  Populate Tables:
    actors:
      ErrorHandler1:
        parent: ErrorHandler
        error: result
        in:
          config:
            const:
            - exceptionKey: java.lang.Exception
              conditions:
                message: ''
              actions:
                suppress: false
                log: true
                flowName: PopulateTableErrorsWithFailed
      Populate Task Execution Entities:
        parent: PopulateTaskExecutionEntities_Actor
        in:
          idType:
            const: ENTITY
      Populate TDM Sequence Mapping:
        parent: PopulateTdmSeqMapping_Actor
  End:
    actors:
      Print Finish Note:
        parent: Logger
        disabled: true
        in:
          message:
            const: Finished Extracting Entity ${luType}.${iid}.
          level:
            const: info
          luType:
            external: luName
            schema: string
            editor:
              id: com.k2view.logicalUnit
              addEmptyEntry: true
            mandatory: false
          iid:
            external: iid
            schema: string
            mandatory: false
  Stage 1: {
    }
  Stage 2: {
    }
  Stage 3: {
    }
  Stage 4: {
    }
