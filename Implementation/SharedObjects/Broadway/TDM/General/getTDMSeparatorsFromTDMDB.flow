tags: TDM
stages:
  TDMDB Schema:
    actors:
      Get TDMDB Schema:
        parent: FabricSetRead
        in:
          key:
            const: TDMDB_SCHEMA
            default: false
  Get IID Separator:
    actors:
      Get IID Separator From TDMDB:
        parent: DbFetchField
        in:
          interface:
            const: TDM
          sql:
            const: "select param_value from ${@schema}.tdm_general_parameters where\
              \ \r\nparam_name ='iid_separator'"
          schema:
            link: Get TDMDB Schema/result
            schema: string
            mandatory: false
        out:
          result:
            schema: string
  Stage 2:
    dependsOn: Get IID Separator
    actors:
      NotNull1:
        parent: NotNull
        condition: result
        in:
          value:
            link: Get IID Separator From TDMDB/result
            schema: string
      Const1:
        parent: Const
        in:
          value:
            const: null
            link: Get IID Separator From TDMDB/result
            schema: string
        out:
          value:
            external: IID_SEPARATOR
            schema: string
    split: '--------------------'
  Stage 3:
    else: true
    transactional: false
    dependsOn: Get IID Separator
    actors:
      Const2:
        parent: Const
        in:
          value:
