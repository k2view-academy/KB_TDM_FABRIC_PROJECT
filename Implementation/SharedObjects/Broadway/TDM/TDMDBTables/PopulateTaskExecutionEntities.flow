tags: TDM
stages:
  Prepare the Data:
    actors:
      Get LU_TYPE:
        parent: FabricSetRead
        in:
          key:
            const: LU_TYPE
            default: false
      Get Entity Status:
        parent: FabricSetRead
        in:
          key:
            const: ENTITY_STATUS
            default: false
      Get TARGET_ENTITY_ID:
        parent: FabricSetRead
        in:
          key:
            const: TARGET_ENTITY_ID
            default: false
      Get IID:
        parent: FabricSetRead
        in:
          key:
            const: IID
            default: false
      Get SOURCE_ENTITY_ID:
        parent: FabricSetRead
        in:
          key:
            const: SOURCE_ENTITY_ID
            default: false
  Continue Preparing:
    actors:
      Get Task Execution ID:
        parent: FabricSetRead
        in:
          key:
            const: TDM_TASK_EXE_ID
            default: false
      Get Timing of Processing Start:
        parent: FabricSetRead
        in:
          key:
            const: IID_START_DATETIME
            default: false
      Get Source Environment:
        parent: FabricSetRead
        in:
          key:
            const: TDM_SOURCE_ENVIRONMENT_NAME
            default: false
      Now1:
        parent: Now
      LuFunction1:
        parent: LuFunction
        in:
          functionName:
            const: fnSplitUID2
          uid:
            link: Get SOURCE_ENTITY_ID/result
            schema: string
            mandatory: false
        out:
          instanceID:
            schema: string
          envName:
            schema: string
          versionExeID:
            schema: string
      Check if Target Entity ID is Not Set:
        parent: Equals
        in:
          a:
            link: Get TARGET_ENTITY_ID/result
            schema: string
          b:
            const: NO_ID
            schema: string
  Check env:
    actors:
      Synthetic:
        parent: EqualsIgnoreCase
        in:
          b:
            const: Synthetic
            schema: string
          a:
            link: Get Source Environment/result
      AI:
        parent: EqualsIgnoreCase
        in:
          b:
            const: AI
            schema: string
          a:
            link: Get Source Environment/result
  'Else ':
    else: true
    transactional: false
    dependsOn: Check env
    split: '--------------------'
  Check if Generate Rule based:
    transactional: false
    dependsOn: Check env
    actors:
      Or:
        parent: Or
        condition: result
        in:
          a:
            link: Synthetic/result
          b:
            link: AI/result
      Default value for version exe id:
        parent: Const
        in:
          value:
            const: 0
            schema: integer
        out:
          value:
            schema: integer
  'Null':
    transactional: false
    dependsOn: 'Else '
    actors:
      Verison execution id is empty:
        parent: EqualsIgnoreCase
        condition: result
        in:
          b:
            const: ''
            schema: string
          a:
            link: LuFunction1/versionExeID
      Default value:
        parent: Const
        in:
          value:
            const: 0
            schema: integer
        out:
          value:
            schema: integer
    split: '--------------------'
  'versionExeID  ':
    else: true
    transactional: false
    dependsOn: 'Else '
    actors:
      versionExeID:
        parent: Const
        in:
          value:
            const: null
            link: LuFunction1/versionExeID
        out:
          value:
            schema: string
      Default value1:
        parent: Const
        in:
          value:
            const: 0
            schema: integer
        out:
          value:
            schema: integer
    split: '--------------------'
  Do Nothing:
    transactional: false
    dependsOn: Check if Generate Rule based
    actors:
      Subset execution id is empty:
        parent: EqualsIgnoreCase
        condition: result
        in:
          b:
            const: ''
            schema: string
          a:
            link: LuFunction1/versionExeID
      'Default value  ':
        parent: Const
        in:
          value:
            const: 0
            schema: integer
        out:
          value:
            schema: integer
    split: '--------------------'
  'subsetExeID  ':
    else: true
    transactional: false
    dependsOn: Check if Generate Rule based
    actors:
      subsetExeID:
        parent: Const
        in:
          value:
            const: null
            link: LuFunction1/versionExeID
        out:
          value:
            schema: string
  Calculate Fabric Get Duration and Total Duration:
    actors:
      Get Clone No:
        parent: FabricSetRead
        in:
          key:
            const: clone_id
            default: false
      Calculate Total Processing Duration:
        parent: DateDuration
        in:
          from:
            link: Get Timing of Processing Start/result
          to:
            link: Now1/date
      Get root_iid:
        parent: FabricSetRead
        in:
          key:
            const: root_iid
            default: false
      Get root_lu_name:
        parent: FabricSetRead
        in:
          key:
            const: root_lu_name
            default: false
      If Target Entity ID is not set, use source ID:
        parent: IfElse
        in:
          a:
            link: Get IID/result
            schema: string
          b:
            link: Get TARGET_ENTITY_ID/result
            schema: string
          test:
            link: Check if Target Entity ID is Not Set/result
        out:
          result:
            schema: string
      Format the Current Time:
        parent: DateFormat
        in:
          format:
            const: yyyy-MMM-dd HH:mm:ss.SSS
          date:
            link: Now1/date
  Load Data to Entities table:
    actors:
      Add Entity to Entities Table:
        parent: DbCommand
        in:
          interface:
            const: TDM
          sql:
            const: "INSERT INTO ${@schema}.TASK_EXECUTION_ENTITIES(TASK_EXECUTION_ID,\
              \ LU_NAME, ENTITY_ID, TARGET_ENTITY_ID, ENV_ID, EXECUTION_STATUS, ID_TYPE,\
              \ \r\nFABRIC_EXECUTION_ID, IID, SOURCE_ENV, CREATION_DATE, ENTITY_START_TIME,\
              \ ENTITY_END_TIME, VERSION_TASK_EXECUTION_ID,SUBSET_TASK_EXECUTION_ID,\
              \ FABRIC_GET_TIME,\r\nTOTAL_PROCESSING_TIME, CLONE_NO, ROOT_ENTITY_ID,\
              \ ROOT_LU_NAME) \r\n(SELECT  ${taskexecutionId}, ${luName}, ${sourceEntityId},\
              \ ${targetEntityId}, L.ENVIRONMENT_ID, ${executionStatus}, ${idType},\
              \ \r\nCOALESCE(FABRIC_EXECUTION_ID, '') AS FABRIC_EXECUTION_ID ,${iid},\
              \ \r\nCASE WHEN T.LOAD_ENTITY = FALSE AND T.DELETE_BEFORE_LOAD = TRUE\
              \ THEN E.ENVIRONMENT_NAME ELSE L.SOURCE_ENV_NAME END,\r\nNow(), ${entityStartTime},\
              \ ${entityEndTime}, ${version_task_execution_id},${subset_task_execution_id},\
              \ ${fabticTime}, \r\n${totalTime}, ${cloneNo}, ${rootEntityId}, ${rootLUName}\r\
              \nFROM ${@schema}.TASK_EXECUTION_LIST L, ${@schema}.TASKS T, ${@schema}.ENViRONMENTS\
              \ E\r\nWHERE TASK_EXECUTION_ID = ${taskexecutionId} AND L.TASK_ID =\
              \ T.TASK_ID AND T.ENViRONMENT_ID = E.ENViRONMENT_ID LIMIT 1) \r\nON\
              \ CONFLICT ON CONSTRAINT task_execution_entities_pkey Do update set\
              \ execution_status = ${executionStatus}"
          luName:
            link: Get LU_TYPE/result
            schema: string
          taskexecutionId:
            link: Get Task Execution ID/result
            schema: string
          sourceEntityId:
            link: Get SOURCE_ENTITY_ID/result
            schema: string
          targetEntityId:
            link: If Target Entity ID is not set, use source ID/result
            schema: string
          executionStatus:
            link: Get Entity Status/result
            schema: string
          iid:
            link: Get IID/result
            schema: string
          entityStartTime:
            link: Get Timing of Processing Start/result
            schema: string
          entityEndTime:
            link: Format the Current Time/string
            schema: string
          fabticTime:
            schema: number
          totalTime:
            link: Calculate Total Processing Duration/millis
            schema: number
          idType:
            external: idType
            schema: string
          cloneNo:
            link: Get Clone No/result
            schema: string
          schema:
            const: '@TDMDB_SCHEMA@'
            schema: string
            mandatory: false
          rootEntityId:
            link: Get root_iid/result
            schema: string
            mandatory: false
          rootLUName:
            link: Get root_lu_name/result
            schema: string
            mandatory: false
          version_task_execution_id:
            link:
            - Default value for version exe id/value
            - Default value/value
            - versionExeID/value
            schema: string
            mandatory: false
          subset_task_execution_id:
            link:
            - Default value/value
            - Default value1/value
            - Default value  /value
            - subsetExeID/value
            schema: string
            mandatory: false
