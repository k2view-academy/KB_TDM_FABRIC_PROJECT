tags: TDM
stages:
  Call Error Flow:
    actors:
      Populate Table Errors:
        parent: PopulateTableErrors
        in:
          error_category:
            const: Entity Failed
          fabricSessionName:
            const: fabric
            schema: string
            editor:
              id: com.k2view.interface
              interfaceType:
              - database
              - CassandraLoader
              interfaces:
              - fabric
            mandatory: false
          error:
            external: error
      Set Entity Status to Failure:
        parent: FabricSet
        in:
          key:
            const: ENTITY_STATUS
            default: false
          value:
            const: failed
            default: false
      Populate Task Execution Entities:
        parent: PopulateTaskExecutionEntities_Actor
        in:
          idType:
            const: ENTITY
