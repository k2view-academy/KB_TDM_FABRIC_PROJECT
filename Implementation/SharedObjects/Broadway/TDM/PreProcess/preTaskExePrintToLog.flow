tags: TDM
stages:
  Get Task Settings:
    actors:
      StringFormat1:
        parent: StringFormat
        in:
          format:
            const: null
            external: message
            remark: Enter the text to be printed to the log upon completion of the
              pre-execution process.
      Sleep1:
        parent: Sleep
        disabled: true
        in:
          millis:
            const: 60000
  Print to Log:
    actors:
      Logger1:
        parent: Logger
        in:
          level:
            const: debug
          message:
            link: StringFormat1/string
