{
	"fabric.UI.schema.showCassandraProperties": false,
	"fabric.UI.schema.showIIDFinderProperties": false,
	"explorer.autoReveal": false,
	"explorer.compactFolders": false,
	"security.workspace.trust.enabled": false,
	"java.server.launchMode": "Standard",
	"java.settings.url": ".theia/org.eclipse.jdt.core.prefs",
	"java.project.referencedLibraries": [
		"/opt/apps/fabric/workspace/fabric/lib/fabric/cdbms-core*.jar",
        "/opt/apps/fabric/workspace/fabric/lib/fabric/cdbms-iid-finder*.jar",
        "/opt/apps/fabric/workspace/fabric/lib/fabric/fabric-session*.jar",
        "/opt/apps/fabric/workspace/fabric/lib/fabric/fabric-common*.jar",
        "/opt/apps/fabric/workspace/fabric/lib/fabric/fabric-api*.jar",
        "/opt/apps/fabric/workspace/fabric/lib/fabric/fabric-parser*.jar",
        "/opt/apps/fabric/workspace/fabric/lib/fabric/fabricdb*.jar",
        "/opt/apps/fabric/workspace/fabric/lib/fabric/broadway*.jar",
		"/opt/apps/fabric/workspace/fabric/lib/fabric/graphit*.jar",
        "/opt/apps/fabric/workspace/fabric/lib/fabric/commons-io-*.jar",
		"/opt/apps/fabric/workspace/fabric/lib/fabric/commons-lang*.jar",
        "/opt/apps/fabric/workspace/fabric/lib/fabric/commons-configuration2*.jar",
        "/opt/apps/fabric/workspace/fabric/lib/fabric/handlebars*.jar",
        "/opt/apps/fabric/workspace/fabric/lib/fabric/tomcat-servlet*.jar",
        "/opt/apps/fabric/workspace/fabric/lib/fabric/jsch*.jar",
        "/opt/apps/fabric/workspace/fabric/lib/fabric/json*.jar",
        "/opt/apps/fabric/workspace/fabric/lib/fabric/kafka*.jar",
        "/opt/apps/fabric/workspace/fabric/lib/fabric/guava*.jar",
		"/opt/apps/fabric/workspace/fabric/lib/fabric/cron-utils*.jar",
        "/opt/apps/fabric/workspace/fabric/lib/provided/*.jar",
        "lib/*.jar"
    ],
    "java.project.outputPath": ".studioWeb/java/bin",
    "java.project.sourcePaths": [
		"Implementation/SharedObjects/Java/src",	
		"Implementation/LogicalUnits/k2_ws/Java/src",	
		"Implementation/LogicalUnits/k2_ref/Java/src",	
		"Implementation/LogicalUnits/Orders/Java/src",	
		"Implementation/LogicalUnits/TDM/Java/src",	
		"Implementation/LogicalUnits/Collection/Java/src",	
		"Implementation/LogicalUnits/TDM_LIBRARY/Java/src",	
		"Implementation/LogicalUnits/TDM_Reference/Java/src"	
	],
	"java.project.resourceFilters": [
        "node_modules",
        ".git",
		".studio",
		".studioWeb",
		"LocalDeploy",
		"LocalDeployWeb",
		"FabricHome"
    ],
	"java.progressReports.enabled": false,
	"files.enableTrash": false,
	"files.exclude": {
		"**/.classpath": true,
		"**/.project": true,
		"**/.settings": true,
		"**/.factorypath": true,
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true,
		"**/._.DS_Store": true,
		"**/ludb.jar": true,
		"**/ludbXMLs.zip": true,
		"**/VirtualDB_Data": true,
		"**/.gitkeep": true,
		"bin": true,
		"out": true,
		".studio": true,
		".studioWeb": true,
		".idea": true,
		".theia": true,
		"*.log.*.gz": true,
		"*.ini.bak*": true,
		"LocalDeploy": true,
		"LocalDeployWeb": true,
		"__actor__editor__temp*.flow": true,
		".svnignore": true,
		"**/Translations": true,
		"**/Parsers": true,
		"**/Jobs": true,
		"**/IidFinder": true,
		"**/Templates": false,
        "**/Environments.k2fabEnv_encrypted.xml": true,
		"ItemsProperties.k2projItems.xml": true,
		"*.k2proj": true,
		"**/Implementation/SharedObjects/Interfaces/Discovery": false
	},
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/**": true,
        ".git": true,
        ".svn": true,
        ".studio": true,
        ".studioWeb": true,
        "LocalDeploy": true,
        "LocalDeployWeb": true,
        "FabricHome": true
    },
	"files.associations": {
		"*.graphit": "json",
		"*.flow": "json",
		"*.actor": "json",
		"*.catalog": "json",
		"*.rdlx-json": "json",
		"*.report": "json",
		"*.discovery": "json",
		"*.template": "handlebars",
		"*.view": "sql",
		"*.out": "log",
        "*.err": "log"
	},
	"workbench.editor.closeOnFileDelete": true,
	"workbench.list.openMode": "doubleClick",
	"npm.packageManager": "npm",
	"debug.javascript.codelens.npmScripts": "never",
	"terminal.integrated.profiles.linux": {
		"bash": {
			"path": "/usr/bin/bash",
			"args": []
		}
	},
	"terminal.integrated.defaultProfile.linux": "bash",
	"redhat.telemetry.enabled": false,
	"logViewer.watch": [
       {
            "groupName": "Fabric",
            "watches": [
                {
                    "title": "k2fabric.log",
                    "pattern": "/opt/apps/fabric/workspace/logs/k2fabric.log"
                },
                {
                    "title": "k2fabric.out",
                    "pattern": "/opt/apps/fabric/workspace/logs/k2fabric.out"
                },
                {
                    "title": "k2fabric.err",
                    "pattern": "/opt/apps/fabric/workspace/logs/k2fabric.err"
                }
            ]
       },
       {
           "groupName": "IID Finder",
           "watches": [
               {
                    "title": "iid_finder.log",
                    "pattern": "/opt/apps/fabric/workspace/logs/iid_finder.log"
               },
               {
                    "title": "iid_finder_failed.log",
                    "pattern": "/opt/apps/fabric/workspace/logs/iid_finder_failed.log"
               }
           ]
       }
    ],
    "logViewer.showStatusBarItemOnChange": false,
    "java.jdt.ls.vmargs": "-XX:+UseParallelGC -XX:GCTimeRatio=4 -XX:AdaptiveSizePolicyWeight=90 -Dsun.zip.disableMemoryMapping=true -Xmx2G -Xms500m",
    "debug.javascript.debugByLinkOptions": "on"
}